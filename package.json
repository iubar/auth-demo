{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "clean:node": "rimraf node_modules",
    "test": "node ./node_modules/jest --watchAll",
    "test-ci": "node ./node_modules/jest --ci",
    "test-update": "node ./node_modules/jest --updateSnapshot  --coverage=false",
    "lint": "node ./node_modules/eslint/bin/eslint.js --ext .js src/",
    "lint-js": "eslint --ext .jsx --ext .js src/",
    "lint-js:fix": "npm run lint-js -- --fix",
    "prettier": "prettier \"src/**/*.js\"",
    "prettier:fix": "npm run prettier  -- --write"
  },
  "dependencies": {
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/material-bottom-tabs": "^5.3.15",
    "@react-navigation/native": "^5.9.4",
    "expo": "^41.0.1",
    "expo-auth-session": "~3.2.3",
    "expo-crypto": "~9.1.0",
    "expo-random": "~11.1.2",
    "expo-secure-store": "~10.1.0",
    "expo-status-bar": "~1.0.4",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-41.0.0.tar.gz",
    "react-native-gesture-handler": "~1.10.3",
    "react-native-paper": "^4.7.0",
    "react-native-reanimated": "~2.1.0",
    "react-native-safe-area-context": "3.2.0",
    "react-native-screens": "~3.0.0",
    "react-native-web": "~0.13.12"
  },
  "devDependencies": {
    "@babel/core": "~7.9.0",
    "babel-preset-expo": "8.3.0",
    "eslint": "^7.18.0",
    "eslint-config-universe": "^7.0.1",
    "jest": "^26.6.3",
    "jest-expo": "^41.0.0",
    "prettier": "^2.2.1",
    "react-test-renderer": "^16.14.0",
    "rimraf": "^3.0.2"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|@sentry/.*)"
    ]
  },
  "eslintConfig": {
    "extends": "universe/native"
  },
  "private": true
}
