{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "clean:node": "rimraf node_modules",
    "test": "node ./node_modules/jest --watchAll",
    "test-ci": "node ./node_modules/jest --ci",
    "test-update": "node ./node_modules/jest --updateSnapshot  --coverage=false",
    "lint": "node ./node_modules/eslint/bin/eslint.js --ext .js src/",
    "lint-js": "eslint --ext .jsx --ext .js src/",
    "lint-js:fix": "npm run lint-js -- --fix",
    "prettier": "prettier \"src/**/*.js\"",
    "prettier:fix": "npm run prettier  -- --write"
  },
  "dependencies": {
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/material-bottom-tabs": "^5.2.17",
    "@react-navigation/native": "^5.7.4",
    "expo": "^39.0.3",
    "expo-auth-session": "~2.0.0",
    "expo-crypto": "~8.3.0",
    "expo-random": "~9.0.0",
    "expo-secure-store": "~9.2.0",
    "expo-status-bar": "~1.0.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-39.0.0.tar.gz",
    "react-native-gesture-handler": "~1.7.0",
    "react-native-paper": "^4.2.0",
    "react-native-reanimated": "~1.13.1",
    "react-native-safe-area-context": "3.1.4",
    "react-native-screens": "~2.10.1",
    "react-native-web": "~0.13.13"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "babel-preset-expo": "^8.3.0",
    "eslint": "^7.9.0",
    "eslint-config-universe": "^4.0.0",
    "jest": "^26.4.2",
    "jest-expo": "^39.0.0",
    "prettier": "^2.1.2",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^3.0.2"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|@sentry/.*)"
    ]
  },
  "eslintConfig": {
    "extends": "universe/native"
  },
  "private": true
}
