{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest",
    "updateSnapshots": "jest -u --coverage=false",
    "lint": "node ./node_modules/eslint/bin/eslint.js --ext .js src/",
    "lint-js": "eslint --ext .jsx --ext .js src/",
    "lint-js:fix": "npm run lint-js -- --fix",
    "prettier": "prettier \"src/**/*.js\"",
    "prettier:fix": "npm run prettier  -- --write"
  },
  "dependencies": {
    "@react-native-community/picker": "^1.6.5",
    "@react-navigation/material-bottom-tabs": "^5.2.15",
    "@react-navigation/native": "^5.7.2",
    "Base64": "^1.1.0",
    "expo": "^38.0.8",
    "expo-auth-session": "~1.4.1",
    "expo-crypto": "~8.2.1",
    "expo-random": "~8.2.1",
    "expo-secure-store": "^9.0.1",
    "expo-status-bar": "^1.0.2",
    "react": "16.11.0",
    "react-dom": "16.11.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-38.0.2.tar.gz",
    "react-native-paper": "^4.0.1",
    "react-native-web": "~0.13.4",
    "react-native-gesture-handler": "~1.6.0",
    "react-native-reanimated": "~1.9.0",
    "react-native-screens": "~2.9.0",
    "react-native-safe-area-context": "~3.0.7",
    "@react-native-community/masked-view": "0.1.10"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "babel-preset-expo": "^8.2.3",
    "eslint": "^7.5.0",
    "eslint-config-universe": "^4.0.0",
    "jest": "^26.1.0",
    "jest-expo": "^38.0.2",
    "prettier": "^2.0.5",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^3.0.2"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|@sentry/.*)"
    ]
  },
  "eslintConfig": {
    "extends": "universe/native"
  },
  "private": true
}
