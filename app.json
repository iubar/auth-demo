{
  "expo": {
    "name": "Auth Demo",
    "slug": "auth-demo",
    "version": "1.0.4",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],	
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {},	
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "mycoolredirect",
    "extra": {
      "eas": {
        "projectId": "9d7f9f0e-98a4-46ed-8d30-732542d69903"
      }
    },
    "owner": "borgo",
    "plugins": [
      "expo-secure-store"
    ]
  }
}
